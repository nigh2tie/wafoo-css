{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Wafoo CSS Configuration",
  "type": "object",
  "properties": {
    "theme": {
      "type": "string",
      "enum": [
        "theme-sakura",
        "theme-koubai",
        "theme-moe",
        "theme-kiku",
        "theme-koori",
        "theme-yanagi",
        "theme-momiji",
        "theme-fuji",
        "theme-uguisu",
        "theme-yuki"
      ],
      "description": "The active color theme for the application."
    },
    "components": {
      "type": "array",
      "items": {
        "type": "string",
        "enum": [
          "Button",
          "Card",
          "Modal",
          "Navbar",
          "Grid",
          "Input",
          "Table"
        ]
      }
    }
  }
}
